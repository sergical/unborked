{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "playwright:install": {
      "cache": false
    },
    "test:checkout": {
      "cache": false
    },
    "traffic": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "seed": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    }
  }
}
